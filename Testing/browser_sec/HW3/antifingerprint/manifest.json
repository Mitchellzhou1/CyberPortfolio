{
  "manifest_version": 2,
  "name": "Canvas Fingerprint Randomizer",
  "version": "1.0",
  "description": "Randomizes canvas fingerprinting to prevent tracking.",
  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
