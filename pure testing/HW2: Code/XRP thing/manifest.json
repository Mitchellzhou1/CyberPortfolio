{
  "manifest_version": 3,
  "name": "XRP to the Moon",
  "description": "Watch XRP Go to the Moon \uD83D\uDCB0\uD83E\uDD11\uD83D\uDCB0 (I made this extension for class)",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/xrp.png",
    "48": "icons/xrp.png",
    "128": "icons/xrp.png"
  },
  "permissions": [],
  "host_permissions": ["https://developer.chrome.com/docs/extensions/*"],
  "content_scripts": [
    {
      "matches": ["https://developer.chrome.com/docs/extensions/*"],
      "js": ["jquery.min.js", "Q1.2/content.js"],
      "run_at": "document_idle"
    }
  ]
}