{
  "name": "Page Title Pinger",
  "description": "Updates all tabs when whenever a tab we have is updated with a new title!",
  "version": "14.828",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Pwn me!"
  },
  "background": {
     "service_worker": "background.js"
   }
}
